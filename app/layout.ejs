            <ul class="list-unstyled d-flex align-items-center justify-content-md-end justify-content-center gap-20 mb-0 fs-3">
              <li><a href="#!"><i class="fa-brands fa-facebook"></i></a></li>
              <li><a href="#!"><i class="fa-brands fa-line"></i></a></li>
            </ul>
          </div>
          <div class="col-md-12 fs-6 fw-bold text-center text-secondary">
            © 2022 Sikdang Restaurant by Iris. All Rights Reserved.
          </div>
        </div>
      </div>
    </footer>
  
    <script src="./assets/js/vendors.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/swiper/swiper-bundle.min.js"></script>
    <% if( current === 'recipe' || current === 'recipes' || current === 'search' ){ %>
      <script src="https://www.gstatic.com/firebasejs/4.12.1/firebase.js"></script>
      <script src="./assets/js/<%- current %>.js" type="module"></script>
    <% } else{ %>
      <script src="./assets/js/all.js"></script>
    <% } %>
  </body>

</html>